<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RotateLoadingView">
        <attr name="loading_width" format="dimension" />
        <attr name="loading_color" format="color" />
        <attr name="shadow_position" format="integer" />
        <attr name="loading_speed" format="integer" />
    </declare-styleable>
    <declare-styleable name="PasswordLayout">
        <attr name="hint" format="string" />
        <attr name="drawableStart" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="LoadingImageView">
        <attr name="custom_enable_remove_image" format="boolean" />
        <attr name="status" format="enum">
            <enum name="read" value="0" />
            <enum name="upload" value="1" />
        </attr>
        <attr name="background" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="android:hint" format="string" />
        <attr name="custom_end_text" format="string" />
        <attr name="custom_start_text" format="string" />
        <attr name="max_number_input" format="float" />
        <attr name="input_type" format="enum">
            <enum name="number" value="0" />
            <enum name="decimal" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomEditTextProfile">
        <attr name="android:hint" />
        <attr name="isPhoneType" format="boolean" />
        <attr name="image_custom" format="reference" />
        <attr name="is_focusable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TopBarMenu">
        <attr name="tbm_title" format="string" />
        <attr name="tbm_isShowOptionMenu" format="boolean" />
        <attr name="tbm_icEnd" format="reference" />
        <attr name="tbm_isShowBack" format="boolean" />
        <attr name="tbm_padding_ic_end" format="dimension" />
    </declare-styleable>

    <declare-styleable name="UserInfoDetailPostView">
        <attr name="uidp_isShowStatus" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StatusOfWorkersView"></declare-styleable>

    <declare-styleable name="JobInfoView"></declare-styleable>

    <declare-styleable name="ItemInfo2LineView">
        <attr name="iil_icon" format="reference" />
        <attr name="iil_showIcon" format="boolean" />
        <attr name="iil_title" format="string" />
        <attr name="iil_value" format="string" />
    </declare-styleable>

    <declare-styleable name="JobDescriptionView">
        <attr name="jd_title" format="string" />
        <attr name="jd_titleTextColor" format="color" />
        <attr name="jd_content" format="string" />
        <attr name="jd_contentTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="InfoUserBookWorkerView">
        <attr name="iubw_title" format="string" />
        <attr name="iubw_titleName" format="string" />
        <attr name="iubw_titlePhoneNumber" format="string" />
    </declare-styleable>

    <declare-styleable name="ShopInfoView">
        <attr name="si_title" format="string" />
    </declare-styleable>

    <declare-styleable name="SalonDescriptionView">
        <attr name="sd_title" format="string" />
        <attr name="sd_description" format="string" />
    </declare-styleable>

    <declare-styleable name="SalonPictureView">
        <attr name="sp_title" format="string" />
    </declare-styleable>

    <declare-styleable name="ServicePriceView">
        <attr name="spv_title" format="string" />
    </declare-styleable>

    <declare-styleable name="CvPictureProfile"></declare-styleable>

    <declare-styleable name="WorkerInfoView">
        <attr name="wiv_title" format="string" />
        <attr name="wiv_iconTopLeft" format="reference" />
        <attr name="wiv_iconTopRight" format="reference" />
        <attr name="wiv_iconBotLeft" format="reference" />
        <attr name="wiv_iconBotRight" format="reference" />
        <attr name="wiv_titleTopLeft" format="string" />
        <attr name="wiv_titleTopRight" format="string" />
        <attr name="wiv_titleBotLeft" format="string" />
        <attr name="wiv_titleBotRight" format="string" />
    </declare-styleable>

    <declare-styleable name="ServicesWorkerView">
        <attr name="sw_title" format="string" />
        <attr name="sw_itemSpace" format="float" />
    </declare-styleable>

    <declare-styleable name="CustomFilterView">
        <attr name="isVisibleExpression" format="boolean" />
        <attr name="isVisibleGender" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomHeaderHome">
        <attr name="hint_search" format="string" />
    </declare-styleable>
</resources>